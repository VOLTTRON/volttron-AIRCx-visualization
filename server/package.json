{
  "name": "aircx-backend",
  "version": "1.0.0",
  "description": "Automatic Identification of Retro-commissioning Measures",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon start",
    "lint": "./node_modules/.bin/eslint --ext .js config/ models/ routes/ seeders/ index.js",
    "lint-fix": "./node_modules/.bin/eslint --fix --ext .js config/ models/ routes/ seeders/ index.js",
    "test": "jest",
    "seed-create": "npx sequelize-cli seed:generate --name",
    "seed-all": "npx sequelize-cli db:seed:all --debug",
    "db-init": "node db-init.js",
    "db-del": "trash db.development.sqlite",
    "reset": "yarn db-del && yarn db-init && yarn seed-all"
  },
  "dependencies": {
    "@mapbox/mbtiles": "^0.11.0",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-jwt-permissions": "^1.3.2",
    "express-session": "^1.15.6",
    "express-winston": "^4.0.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "sequelize": "^5.3.1",
    "sequelize-cli": "^5.4.0",
    "sqlite3": "^4.1.0",
    "supertest": "^4.0.2",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-prettier": "^2.7.0",
    "faker": "^4.1.0",
    "jest": "^25.4.0",
    "prettier": "^1.16.1",
    "trash-cli": "^3.0.0"
  },
  "proxy": "http://localhost:3001",
  "repository": "https://stash.pnnl.gov/scm/uwtechint/parking.git",
  "author": "Amelia Bleeker <amelia.bleeker@pnnl.gov>"
}
